    "../../frontend/node_modules/@nuxt/telemetry/node_modules",
    "../../frontend/dist",
    "../../frontend/.data",
    "../../frontend/app/**/*",
    "../../frontend/modules/*/runtime/**/*",
    "../../frontend/layers/*/app/**/*",
    "../../frontend/layers/*/modules/*/runtime/**/*",
    "../../frontend/modules/*/runtime/server/**/*",
    "../../frontend/layers/*/server/**/*",
    "../../frontend/layers/*/modules/*/runtime/server/**/*",
    "../../frontend/node_modules/runtime",
    "../../frontend/node_modules/dist/runtime"
