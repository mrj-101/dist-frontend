{
  "compilerOptions": {
    "paths": {
      "nitropack/types": [
        "../../frontend/node_modules/nitropack/types"
      ],
      "nitropack/runtime": [
        "../../frontend/node_modules/nitropack/runtime"
      ],
      "nitropack": [
        "../../frontend/node_modules/nitropack"
      ],
      "defu": [
        "../../frontend/node_modules/defu"
      ],
      "h3": [
        "../../frontend/node_modules/h3"
      ],
      "consola": [
        "../../frontend/node_modules/consola"
      ],
      "ofetch": [
        "../../frontend/node_modules/ofetch"
      ],
      "@nuxt/devtools": [
        "../../frontend/node_modules/@nuxt/devtools"
      ],
      "@vue/runtime-core": [
        "../../frontend/node_modules/@vue/runtime-core"
      ],
      "@vue/compiler-sfc": [
